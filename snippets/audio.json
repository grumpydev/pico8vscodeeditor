{
    "audio.music": {
        "prefix": "music",
        "scope": "pico8p8",
        "body": [
          "music(${1:pattern}${2:,optional_fade_ms}${3:,optional_channel_mask})",
          "$0"
        ],
        "description": "Play music (-1 = stop)"
    },
    "audio.sfx": {
        "prefix": "sfx",
        "scope": "pico8p8",
        "body": [
          "sfx(${1:sfx_number}${2:,optional_channel}${3:,optional_offset})",
          "$0"
        ],
        "description": "Play sfx"
    }
}